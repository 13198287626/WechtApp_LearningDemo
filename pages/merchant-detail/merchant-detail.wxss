.merchant-img {
    width: 100%;
    height: 620rpx;
    background-size: cover;
    background-position: 50%;
}
.logoarea {
    position: relative;
    width: 100%;
    height: 160rpx;
    margin-top: -160rpx;
    background: -webkit-linear-gradient(rgba(0,0,0,0), rgba(0,0,0,.99));
    background: -o-linear-gradient(rgba(0,0,0,0), rgba(0,0,0,.99));
    background: -moz-linear-gradient(rgba(0,0,0,0), rgba(0,0,0,.99));
    background: linear-gradient(rgba(0,0,0,0), rgba(0,0,0,.99));
}
.logoarea .logo {
    display: inline-block;
    width: 160rpx;
    height: 160rpx;
    margin: 36rpx 30rpx 0;
    border-radius: 160rpx;
    background-color: #f5f5f5;
    background-size: 160rpx;
    background-repeat: no-repeat;
    background-position: 50%;
}
.logoarea .title {
    display: inline-block;
    position: absolute;
    width: 100%;
    left: 0;
    bottom: 18rpx;
    font-size: 36rpx;
    font-weight: 500;
    color: #fff;
    text-indent: 222rpx;
    line-height: 72rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.logoarea .lvka {
    display: inline-block;
    width: 68rpx;
    height: 36rpx;
    margin-left: -108rpx;
    background-image: url("data:image/png;base64,");
    background-size: 68rpx 36rpx;
}

.descarea {
    padding: 60rpx 30rpx 40rpx;
    font-size: 24rpx;
    font-weight: normal;
    line-height: 44rpx;
    text-indent: 2em;
    color: #475966;
    background: #fff;
    margin-bottom: 20rpx;
}
.storelist .store {
    position: relative;
    margin-bottom: 20rpx;
    background-color: #fff;
}
.storelist .store:before {
    display: inline-block;
    position: absolute;
    content: "";
    top: 140rpx;
    width: 100%;
    height: 1px;
    background-color: #eaeaea;
    -webkit-transform: scaleY(.5);
        -moz-transform: scaleY(.5);
            transform: scaleY(.5);
}
.storelist .store:after {
    display: inline-block;
    position: absolute;
    content: "";
    top: 240rpx;
    height: 1px;
    width: 100%;
    background-color: #eaeaea;
    -webkit-transform: scaleY(.5);
        -moz-transform: scaleY(.5);
            transform: scaleY(.5);
}

.storelist .title {
      height: 140rpx;
      line-height: 140rpx;
      font-size: 34rpx;
      font-weight: 500;
      color: #1a2833;
      padding: 0 30rpx;
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
    }
.storelist .list-item {
    position: relative;
    height: 100rpx;
}
.storelist .list-item .icon {
    display: inline-block;
    width: 36rpx;
    height: 100rpx;
    margin: 0 20rpx 0 30rpx;
    background-repeat: no-repeat;
    background-size: 36rpx;
    background-position: 50%;
}
.storelist .list-item .text {
    position: absolute;
    left: 0;
    width: 100%;
    text-indent: 86rpx;
    display: inline-block;
    height: 100rpx;
    line-height: 100rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
    
.storelist .addr-icon {
      background-image: url("data:image/png;base64,");
}
.storelist .phone-icon {
      background-image: url("data:image/png;base64,");
}
.storelist .addr-text {
    font-size: 26rpx;
    font-weight: normal;
    color: #475966;
}
.storelist .phone-text {
    font-size: 32rpx;
    font-weight: 700;
    color: #ffc600;
}
